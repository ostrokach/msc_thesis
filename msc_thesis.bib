  shortjournal = {Nucl. Acids Res.},
  author = {Rattei, Thomas and Tischler, Patrick and G{\"o}tz, Stefan and Jehl, Marc-Andr{\'e} and Hoser, Jonathan and Arnold, Roland and Conesa, Ana and Mewes, Hans-Werner},
  urldate = {2015-04-07},
  date = {2010-01-01},
  pages = {D223--D226},
  note = {00031},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/UIA3AUFR/Rattei et al. - 2010 - SIMAP—a comprehensive database of pre-calculated p.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/K2IH9TZF/D223.html:text/html},
  eprinttype = {pmid},
  eprint = {19906725}
}

@article{zhu_noxclass:_2006,
