  urldate = {2016-04-16},
  date = {2015-11-03},
  pages = {e1004556},
  keywords = {Molecular evolution,Multiple alignment calculation,Mutation databases,Point mutation,protein structure,Sequence Alignment,Sequence databases,Substitution mutation},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/AH9SDQAV/Bednar et al. - 2015 - FireProt Energy- and Evolution-Based Computationa.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/QFDDTBPC/article.html:text/html}
}

@article{wells_reaching_2007,
  title = {Reaching for high-hanging fruit in drug discovery at protein\textendash{}protein interfaces},
  volume = {450},
  rights = {\textcopyright{} 2007 Nature Publishing Group},
  issn = {0028-0836},
  doi = {10.1038/nature06526},
  abstract = {Targeting the interfaces between proteins has huge therapeutic potential, but discovering small-molecule drugs that disrupt protein\textendash{}protein interactions is an enormous challenge. Several recent success stories, however, indicate that protein\textendash{}protein interfaces might be more tractable than has been thought. These studies discovered small molecules that bind with drug-like potencies to 'hotspots' on the contact surfaces involved in protein\textendash{}protein interactions. Remarkably, these small molecules bind deeper within the contact surface of the target protein, and bind with much higher efficiencies, than do the contact atoms of the natural protein partner. Some of these small molecules are now making their way through clinical trials, so this high-hanging fruit might not be far out of reach.},
