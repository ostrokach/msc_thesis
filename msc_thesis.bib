  urldate = {2016-09-11},
  date = {2014},
  pages = {5:1--5:9},
  file = {ACM Full Text PDF:/home/kimlab1/strokach/zotero/storage/UTK593SF/He et al. - 2014 - Practical Lessons from Predicting Clicks on Ads at.pdf:application/pdf}
