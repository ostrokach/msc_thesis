  urldate = {2016-04-13},
  date = {2010-05-01},
  pages = {464--473},
  keywords = {CBAVD,CFTR,CFTR-related disease,cystic fibrosis,disease severity,missense mutation,pancreatic insufficiency,PANTHER,PolyPhen,SIFT},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/KBGM2FCV/Dorfman et al. - 2010 - Do common in silico tools predict the clinical con.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/HFAVQPFD/full.html:text/html}
}

@article{petukh_predicting_2015,
  title = {Predicting {{Binding Free Energy Change Caused}} by {{Point Mutations}} with {{Knowledge-Modified MM}}/{{PBSA Method}}},
  volume = {11},
  issn = {1553-7358},
  doi = {10.1371/journal.pcbi.1004276},
  abstract = {Author Summary   Developing methods for accurate prediction of effects of amino acid substitutions on protein-protein affinity is important for both understanding disease-causing mechanism of missense mutations and guiding protein engineering. For both purposes, there is a need for accurate methods primarily based on first principle calculations, while being fast enough to handle large number of cases. Here we report a new method, the Single Amino Acid Mutation based change in Binding free Energy (SAAMBE) method. The core of the SAAMBE method is a modified molecular mechanics Poisson-Boltzmann Surface Area (MM/PBSA) method with residue specific dielectric constant. Adopting residue specific dielectric constant allows for mimicking the effects of plausible conformational changes induced by the binding on the solvation energy without performing computationally expensive explicit modeling. This makes the SAAMBE algorithm fast, while still capable of capturing many of the explicit effects associated with the binding. The performance of the SAAMBE protocol was tested against experimentally determined binding free energy changes over 1300 mutations in 43 proteins and very good correlation coefficient was obtained. Due to its computational efficiency, the SAAMBE method will be soon implemented into webserver and made available to the computational community.},
  timestamp = {2016-04-16T22:16:17Z},
  number = {7},
  journaltitle = {PLOS Comput Biol},
  shortjournal = {PLOS Comput Biol},
  author = {Petukh, Marharyta and Li, Minghui and Alexov, Emil},
  urldate = {2016-04-16},
  date = {2015-07-06},
  pages = {e1004276},
  keywords = {Algorithms,Dielectrics,Electrostatics,Free energy,Hydrogen bonding,Mutation databases,Point mutation,Substitution mutation},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/CEXK5S29/Petukh et al. - 2015 - Predicting Binding Free Energy Change Caused by Po.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/57VQX2HA/article.html:text/html}
}

@article{pires_mcsm:_2014,
  title = {{{mCSM}}: predicting the effects of mutations in proteins using graph-based signatures},
  volume = {30},
  issn = {1367-4803, 1460-2059},
  doi = {10.1093/bioinformatics/btt691},
  shorttitle = {{{mCSM}}},
  abstract = {Motivation: Mutations play fundamental roles in evolution by introducing diversity into genomes. Missense mutations in structural genes may become either selectively advantageous or disadvantageous to the organism by affecting protein stability and/or interfering with interactions between partners. Thus, the ability to predict the impact of mutations on protein stability and interactions is of significant value, particularly in understanding the effects of Mendelian and somatic mutations on the progression of disease. Here, we propose a novel approach to the study of missense mutations, called mCSM, which relies on graph-based signatures. These encode distance patterns between atoms and are used to represent the protein residue environment and to train predictive models. To understand the roles of mutations in disease, we have evaluated their impacts not only on protein stability but also on protein\textendash{}protein and protein\textendash{}nucleic acid interactions.
Results: We show that mCSM performs as well as or better than other methods that are used widely. The mCSM signatures were successfully used in different tasks demonstrating that the impact of a mutation can be correlated with the atomic-distance patterns surrounding an amino acid residue. We showed that mCSM can predict stability changes of a wide range of mutations occurring in the tumour suppressor protein p53, demonstrating the applicability of the proposed method in a challenging disease scenario.
Availability and implementation: A web server is available at http://structure.bioc.cam.ac.uk/mcsm.
Contact: dpires\{at\}dcc.ufmg.br; tom\{at\}cryst.bioc.cam.ac.uk
Supplementary information: Supplementary data are available at Bioinformatics online.},
  timestamp = {2016-04-16T22:16:46Z},
  langid = {english},
  number = {3},
  journaltitle = {Bioinformatics},
  shortjournal = {Bioinformatics},
  author = {Pires, Douglas E. V. and Ascher, David B. and Blundell, Tom L.},
  urldate = {2016-04-16},
  date = {2014-01-02},
  pages = {335--342},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/SFKQW6GH/Pires et al. - 2014 - mCSM predicting the effects of mutations in prote.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/KAZ7QZKT/335.html:text/html},
  eprinttype = {pmid},
  eprint = {24281696}
}

@article{o_conchuir_web_2015,
  title = {A {{Web Resource}} for {{Standardized Benchmark Datasets}}, {{Metrics}}, and {{Rosetta Protocols}} for {{Macromolecular Modeling}} and {{Design}}},
  volume = {10},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0130433},
  abstract = {The development and validation of computational macromolecular modeling and design methods depend on suitable benchmark datasets and informative metrics for comparing protocols. In addition, if a method is intended to be adopted broadly in diverse biological applications, there needs to be information on appropriate parameters for each protocol, as well as metrics describing the expected accuracy compared to experimental data. In certain disciplines, there exist established benchmarks and public resources where experts in a particular methodology are encouraged to supply their most efficient implementation of each particular benchmark. We aim to provide such a resource for protocols in macromolecular modeling and design. We present a freely accessible web resource (https://kortemmelab.ucsf.edu/benchmarks) to guide the development of protocols for protein modeling and design. The site provides benchmark datasets and metrics to compare the performance of a variety of modeling protocols using different computational sampling methods and energy functions, providing a "best practice" set of parameters for each method. Each benchmark has an associated downloadable benchmark capture archive containing the input files, analysis scripts, and tutorials for running the benchmark. The captures may be run with any suitable modeling method; we supply command lines for running the benchmarks using the Rosetta software suite. We have compiled initial benchmarks for the resource spanning three key areas: prediction of energetic effects of mutations, protein design, and protein structure prediction, each with associated state-of-the-art modeling protocols. With the help of the wider macromolecular modeling community, we hope to expand the variety of benchmarks included on the website and continue to evaluate new iterations of current methods as they become available.},
  timestamp = {2016-04-21T17:11:41Z},
  number = {9},
  journaltitle = {PloS One},
  shortjournal = {PLoS ONE},
  author = {{\'O} Conch{\'u}ir, Shane and Barlow, Kyle A. and Pache, Roland A. and Ollikainen, Noah and Kundert, Kale and O'Meara, Matthew J. and Smith, Colin A. and Kortemme, Tanja},
  date = {2015},
  pages = {e0130433},
  keywords = {Alanine,Crystal structure,Macromolecular design,Multiple alignment calculation,Point mutation,Protein sequencing,protein structure,protein structure prediction},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/ZHGA5PJ6/Conchúir et al. - 2015 - A Web Resource for Standardized Benchmark Datasets.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/V89EKMW5/article.html:text/html},
  eprinttype = {pmid},
  eprint = {26335248},
  pmcid = {PMC4559433}
}

