  pages = {2816--2821},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/TKDNZXE6/Fariselli et al. - 2015 - INPS predicting the impact of non-synonymous vari.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/W3MK2QIJ/2816.html:text/html},
