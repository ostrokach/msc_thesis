  author = {Khan, Sofia and Vihinen, Mauno},
  urldate = {2016-04-16},
  date = {2010-06-01},
  pages = {675--684},
  keywords = {Bioinformatics,computational methods,Free energy,missense mutations,prediction programs,predictions,Protein stability,stability predictors},
  file = {Full Text PDF:/home/kimlab1/strokach/zotero/storage/6N4BSKZI/Khan and Vihinen - 2010 - Performance of protein stability predictors.pdf:application/pdf;Snapshot:/home/kimlab1/strokach/zotero/storage/W5KNV6UR/abstract.html:text/html}
}

@article{fariselli_inps:_2015,
